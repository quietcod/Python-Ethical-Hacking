{
  "tool_configurations": {
    "nmap": {
      "binary_path": "nmap",
      "common_args": ["-sS", "-Pn", "--disable-arp-ping"],
      "stealth_args": ["-sS", "-T2", "-f"],
      "aggressive_args": ["-sS", "-T4", "-A", "-O"],
      "port_ranges": {
        "quick": "--top-ports 1000",
        "full": "-p-",
        "web": "-p 80,443,8080,8443,8000,8888"
      }
    },
    
    "masscan": {
      "binary_path": "masscan", 
      "default_rate": "1000",
      "max_rate": "10000",
      "exclude_ports": "0"
    },
    
    "subfinder": {
      "binary_path": "subfinder",
      "config_file": "~/.config/subfinder/config.yaml",
      "sources": [
        "alienvault", "anubis", "bufferover", "cebaidu", "certspotter",
        "chaos", "chinaz", "dnsdb", "hackertarget", "passivetotal",
        "securitytrails", "shodan", "spyse", "threatbook", "virustotal"
      ]
    },
    
    "nikto": {
      "binary_path": "nikto",
      "config_file": "/etc/nikto.conf",
      "default_plugins": "ALL",
      "timeout": 600
    },
    
    "gobuster": {
      "binary_path": "gobuster",
      "wordlists": {
        "common": "/usr/share/wordlists/dirb/common.txt",
        "big": "/usr/share/wordlists/dirb/big.txt", 
        "directory": "/usr/share/seclists/Discovery/Web-Content/directory-list-2.3-medium.txt"
      },
      "extensions": ["php", "html", "txt", "js", "css", "xml", "json"]
    },
    
    "sslscan": {
      "binary_path": "sslscan",
      "check_certificates": true,
      "check_ciphers": true,
      "check_protocols": true
    },
    
    "nuclei": {
      "binary_path": "nuclei",
      "templates_path": "~/nuclei-templates",
      "update_templates": true,
      "severity_filter": ["critical", "high", "medium", "low", "info"]
    }
  }
}
